<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.xiaowu.myself"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".app.base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_myself"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_myself"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FirstActivity" android:theme="@style/AppThemeNoTitle">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>